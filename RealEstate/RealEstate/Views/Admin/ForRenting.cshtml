@{
    ViewData["Title"] = "Admin Renting Page";
    Layout = "_Layout";
}


<table class="table">
    <thead>
        <tr>

            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Alan</th>
            <th scope="col">Oda Sayısı</th>
            <th scope="col">Fiyat</th>
            <th scope="col">Konum</th>
            <th scope="col">Durum</th>
            

        </tr>
    </thead>
    <tbody>
        @foreach (HouseViewModel item in @ViewBag.ev)
        {
            <tr>

                <th scope="row">@item.Id</th>
                <td>@item.Title</td>
                <td>@item.Square</td>
                <td>@item.RoomNumber</td>
                <td>@item.Price</td>
                <td>@item.LocationName</td>
                <td>@item.Statu</td>
               
               

            </tr>
        }
    </tbody>
</table>
