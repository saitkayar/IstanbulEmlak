@model List< District>
@{
    ViewData["Title"] = "District";
    Layout = "_Layout";
}

<table class="table">
    <thead>
        <tr>

            <th scope="col">#</th>
            <th scope="col">İlçeler</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <th scope="row">@item.Id</th>
                <td>@item.LocationName</td>

            </tr>
        }
    </tbody>
</table>

